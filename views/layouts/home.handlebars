<div class="header" id="myHeader">
  <span class = "spacer"></span>
  <div class="home">
    <h1><a href = '/home/'>Home</a></h1>
  </div>
  <span class = "spacer"></span>
  <div class="filter">
    <h2><a href = '/filters/'>Filters</a></h2>
  </div>
  <span class = "spacer"></span>
  <div class = "Files">
    <h2><a href = '/image/'>Files</a></h2>
  </div>
  <span class = "spacer"></span>
  <div class="login">
    {{#if logged}}
        <h2><a href = '/profile/'>{{username}}</a></h2>
    {{else}}
        <h2><a href = '/profile/'>Log in</a></h2>
    {{/if}}
  </div>
  <span class = "spacer"></span>
</div>
<div class="content">
        <p class = "welcome">Welcome to PSLite!</p>
        {{#each imgarray}}
            <p><a href = "/images/"+{{id}}>{{name}}</a></p>
        {{/each}}
</div>